#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char name[50] ;
    int age;
    float cgpa;
    char grade ;
    
    
scanf("%s", name);
    scanf("%d", &age);
     scanf("%f", &cgpa);
     scanf(" %c", &grade);
    
    int scaled_cgpa = cgpa * 100;
    
    printf("Name: %s\n", name);
    printf("Age: %d\n", age);
    printf("CGPA: %.2f\n", (float)scaled_cgpa / 100.0); 
printf("Grade: %c\n",grade);
            
    
    return 0;
}
