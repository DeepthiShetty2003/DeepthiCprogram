#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int team;
    int ppl;
    int assit;
    scanf("%d", &ppl);
    scanf("%d", &team);
    int div = ppl/team;
    assit = ppl%team ;
    printf("The number of friends in each team is %d and left out is %d", div, assit);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
