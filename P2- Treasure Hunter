#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int x, y, z;
scanf("%d", &x);
    scanf("%d", &y);
    scanf("%d", &z);
    
    int X = x*y*0.01;
    int ans = x - X;
    int Y = ans*z*0.01;
    int R = x - X -Y ;
    int div = R/3;
    printf("%d\n", X);
    printf("%d\n", Y);
    printf("%d\n", div);
    
    return 0;
}
