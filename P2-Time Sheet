#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int d1, d2, d3, d4, d5, d6, d7;
    scanf("%d",&d1);
   scanf("%d",&d2);
    scanf("%d",&d3);
    scanf("%d",&d4);
    scanf("%d",&d5);
    scanf("%d",&d6);
    scanf("%d",&d7);
    
    int c1,c2,c3,c4,c6,c5,c7; 
    
    {  
        if(d1<=8)
     {
        c1= (d1*100)+(0.5*d1*100);    }
    if (d1>8)
    { c1=(((d1-8)*15)+(d1*100))+(0.5*d1*100);
    }
    
    if(d2<=8)
     {
        c2= (d2*100);    }
    if (d1>8)
    { c2=(((d2-8)*15)+(d2*100));
    }
    
     if(d3<=8)
     {
        c3= (d3*100);    }
    if (d3>8)
    { c3=(((d3-8)*15)+(d3*100));
    }
    
     if(d4<=8)
     {
        c4= (d4*100);    }
    if (d4>8)
    { c4=(((d4-8)*15)+(d4*100));
    }
    
     if(d5<=8)
     {
        c5= (d5*100);    }
    if (d5>8)
    { c5=(((d5-8)*15)+(d5*100));
    }
    
     if(d6<=8)
     {
        c6= (d6*100);    }
    if (d6>8)
    { c6=(((d6-8)*15)+(d6*100));
    }
    
    if(d7<=8)
     {
        c7= (d7*100)+(0.25*d7*100);    }
    if (d7>8)
    { c7=(((d7-8)*15)+(d7*100))+(0.25*d7*100);
    }
}
   int total = c1 + c2 + c3 + c4 + c5 + c6 + c7;
    printf("%d", total);
    
    return 0;
}
