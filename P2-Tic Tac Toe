#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int y;
    scanf("%d",&y);
    
    
    if(y==1)
    { printf("0 0\n");}
    else if(y==2)
    {  printf("0 1\n");}
    else if(y==3)    
    {   printf("0 2\n");}
       else if(y==4)
             { printf("1 0\n");}
           else if(y==5)
                {  printf("1 1\n");}
                else if(y==6)
                  {    printf("1 2\n");}
                  else if(y==7)
                    {      printf("2 0\n");}
                        else if(y==8)
                      {        printf("2 1\n");}
                  else if(y==9)
                        {  printf("2 2\n");
}                       

                                                                                    
   
                            
        
     
    return 0;
}


