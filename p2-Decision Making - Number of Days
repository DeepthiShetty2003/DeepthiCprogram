#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num,m, d;
    scanf("%d", &num);
    scanf("%d", &m);
    
    if(m==2)
    {
        if((num%4==0 && num%100!=0)||num%400==0)
            d= 29;
        else
            d=28;
        
    }
    
    else if(m==1||m==3||m==5||m==7||m==8||m==10||m==12)
        d=31;
    
    else
        d=30;
        
printf("%d Days",d)  ;
    return 0;
}
