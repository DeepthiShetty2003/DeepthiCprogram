#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int unit, a, b, c, d;
    scanf("%d", &unit);
    if(unit <= 200)
    {
        a = unit * 0.5;
        printf("Rs.%d",a);
    }
    else if(unit <= 400)
    {
        b = (unit*0.65)+100;
        printf("Rs.%d",b);
    }
    else if(unit <=600)
    {
        c = (unit*0.80)+200;
        printf("Rs.%d",c);
    }
    else
    {
        d = (unit*1.25)+425;
        printf("Rs.%d",d);
    }
    
    return 0;
}
