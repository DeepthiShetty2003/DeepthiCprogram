#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int num, a, b;
       scanf("%d", &num);
       if(100>num || num>999)
       {
           printf("Invalid Number");
           return 0;
       }
          
  else
  { a = num/10;
    b =a%10;     
    { 
        if(b%3==0)
        { printf("Trendy Number");
    }
               else
               { printf("Not a Trendy Number");}  
    }
  }
    return 0;
}

